{
    "type": "symfony-bundle",
    "license": "proprietary",
    "name": "douglas/captcha-bundle",
    "description": "Captcha bundle",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "friendsofsymfony/user-bundle": "~2.0",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Douglas\\CaptchaBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Douglas\\CaptchaBundle\\Tests\\": "tests"
        }
    },
    "replace": {
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
